<Window x:Class="ExamTwoQuestions.PointOfSale.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamTwoQuestions.PointOfSale"
        xmlns:data="clr-namespace:ExamTwoCodeQuestions.Data;assembly=ExamTwoCodeQuestions.Data"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <data:Cobbler/>
    </Window.DataContext>
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource= "https://upload.wikimedia.org/wikipedia/commons/thumb/b/b7/Peach_Cobbler.jpg/2560px-Peach_Cobbler.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <local:CustomizeCobblerControl/>
    </Grid>

</Window>
